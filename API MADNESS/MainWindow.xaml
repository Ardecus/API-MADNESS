<Window x:Class="API_MADNESS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:API_MADNESS"
        mc:Ignorable="d"
        Title="Notifier" Height="350" Width="525">
    <Grid>
        <Expander Header="Codeforces" IsExpanded="True">
            <StackPanel x:Name="CodeforcesList" Orientation="Vertical">
                <Border>
                    <StackPanel Orientation="Vertical"/>
                </Border>
            </StackPanel>
        </Expander>
    </Grid>
</Window>
